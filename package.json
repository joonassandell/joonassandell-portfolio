{
  "name": "joonassandell-portfolio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.1.7",
  "scripts": {
    "analyze": "ANALYZE=true bun run build",
    "build": "next build",
    "build:update": "bun update @joonassandell/ui-lab && bun run build",
    "build:ignore": "IGNORE_ERRORS=true bun run build",
    "check": "bun run lint && bun run typecheck",
    "create:component": "bun --bun ./scripts/create-component.cjs",
    "dev": "next dev",
    "lint:fix": "next lint --fix",
    "lint": "next lint",
    "preview": "bun run build && bun run start",
    "preview:ignore": "bun run build:ignore && bun run start",
    "start": "next start",
    "typecheck": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@joonassandell/ui-lab": "^0.1.7",
    "@next/third-parties": "^14.2.4",
    "@nivo/line": "^0.86.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.22",
    "focus-trap-react": "^10.2.3",
    "framer-motion": "10.16.16",
    "glob": "^10.3.12",
    "locomotive-scroll": "^4.1.4",
    "lodash-es": "^4.17.21",
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "^17.5.0",
    "sharp": "0.33.3",
    "use-callback-ref": "^1.3.2",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.3",
    "@svgr/webpack": "^8.1.0",
    "@types/locomotive-scroll": "^4.1.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.8",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.19",
    "colors": "^1.4.0",
    "eslint": "^8",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.2.5",
    "sass": "^1.76.0",
    "sass-mq": "^6.0.0",
    "typescript": "^5"
  },
  "engines": {
    "node": "20.x"
  }
}
